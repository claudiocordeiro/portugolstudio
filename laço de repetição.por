programa
{
	funcao inicio()
	{
		inteiro i = 1
		enquanto(i <= 100){
			escreva (i + ", ")
			i++ 
		}
		escreva("\n\n fim do laço enquanto\n\n")
		para(inteiro j = 100 ; j>=0 ; j--){
		     	   escreva(j+", ")
	    }
	    escreva("\n\nfim do laçopara")
	}
}
